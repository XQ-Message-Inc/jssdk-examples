{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": 0
    },
    "ignorePatterns": ["dist", "node_modules"]
}
  